{
  "name": "ojt-mandiri-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "dev": "env-cmd -f./.env pm2 start ./bin/www --watch --name 'OJT-API-DEV'",
    "reload": "pm2 delete OJT-API-DEV && yarn dev",
    "prod": "env-cmd -f./.env.prod pm2 start ./bin/www --name 'OJT-API-PROD'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "~4.16.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "sequelize-cli": "^6.5.2"
  }
}
